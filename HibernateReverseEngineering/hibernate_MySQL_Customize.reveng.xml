<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name=".*" match-catalog="db_hibernate_demo">
	</table-filter>
	<table catalog="db_hibernate_demo" name="worker">
		<column name="NAME">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
		</column>
		<column name="MONTHLY_PAY">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
		</column>
		<column name="YEARLY_PAY">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
		</column>
		<column name="VOCATION_WITH_PAY">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
		</column>
	</table>
</hibernate-reverse-engineering>